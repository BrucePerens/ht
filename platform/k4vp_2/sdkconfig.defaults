CONFIG_ESP32_REV_MIN_3_1=y
CONFIG_ESP_CONSOLE_UART_BAUDRATE=460800
CONFIG_ESP_DEFAULT_CPU_FREQ_MHZ_240=y
CONFIG_ESP_HTTPS_SERVER_ENABLE=y
CONFIG_ESP_MAIN_TASK_STACK_SIZE=10240
